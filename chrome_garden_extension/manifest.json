{
	"manifest_version": 3,
	"name": "Garden tab",
	"version": "1.0",
	"description": "Brighten your day with a little garden in your new tabs.",
	"permissions": [
		"storage",
		"activeTab",
		"scripting",
		"history"
	],
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"action": {
		"default_popup": "popup.html"
	}
}
